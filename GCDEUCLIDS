public class GCD {


    public static int  gcd(int a , int b) {
        // if(a%b==0) {
        //     return b;
        // }

        // if(b%a==0) {
        //     return a;
        // }
        if(b==0) {
            return a;
        }
        return gcd(b, a%b);
    }
    public static void main(String[] args) {
        int a  = 24;
        int b= 43;
        int g_cd = gcd(a,b);
        System.out.println(g_cd);
    }
}
